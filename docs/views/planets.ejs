<%- include('partials/head') %>

<h1> Planets </h1>
<section>
<% for(let i=0; i < planetsData.length; i++) { %>
<article>
    <a href="planets/<%= planetsData[i].id %>">
    <ul>
        <li>Naam: <%= planetsData[i].name %></li>
        <li>Diameter: <%= planetsData[i].diameter %> </li>
        <li>Klimaat: <%= planetsData[i].climate %> </li>
    </ul>
</a>
</article>
<% } %>
</section>

