<%- include('partials/head') %>

<h1> Verhicles </h1>
<section>
    <% for(let i=0; i < Data.length; i++) { %>
    <a href="/vehicles/<%= Data[i].id %>">
        <article>
            <ul>
                <li>Naam: <%= Data[i].name %></li>
                <li>Crew: <%= Data[i].crew %> </li>
